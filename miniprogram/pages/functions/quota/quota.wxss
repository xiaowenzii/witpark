page {
  height: 100%;
  width: 100%;
  background-color: #F2F3F3;
}

.view-text{
  font-size: 24rpx;
  margin: 6rpx 12rpx;
}

.head-desc{
  width: 33%;
  display: flex;
  align-items: center;
  flex-direction: row;
  justify-content: center;
}
.head-image{
  height: 18rpx;
  width: 24rpx;
  margin-top: 6rpx;
  margin-left: 12rpx;
}
.head-desc-text1{
  font-size: 24rpx;
  margin-left: 9rpx;
}
/* 列表样式 */
.view-item{
  padding: 12rpx;
  height: 250rpx;
  font-size: 24rpx;
  background-color: #FFFFFF;
  margin-top: 24rpx;
}

.item-but{
  margin: auto 12rpx;
  border: 1rpx solid #F5F5F5;
  padding: 6rpx 24rpx;
}

/* Dialog 样式 */
.condtion-dialog{
  width: 100%;
  background-color: #00000020;
  position: absolute;
  z-index: 10;
  top: 0;
  left: 0;
}