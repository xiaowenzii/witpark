page{
  width: 100%;
  height: 100%;
  background-color: #FFFFFF;
}
.head-desc{
  background-color: #FFFFFF;
  display: flex;
  align-items: center;
  flex-direction: row;
  justify-content: center;
  position: absolute;
  right: 0rpx;
  margin-right: 24rpx;
  padding: 3rpx 9rpx;
  border-radius: 6rpx;
}
.head-image{
  height: 18rpx;
  width: 24rpx;
  margin-top: 6rpx;
  margin-left: 12rpx;
}
.head-desc-text1{
  font-size: 24rpx;
  margin-left: 9rpx;
}
.date-select-view{
  height: 64rpx;
  padding: 12rpx 24rpx;
  display: flex;
  flex-direction: row;
  position: relative;
  align-items: center;
}

.date-view{
  font-size: 30rpx;
  padding: 9rpx 18rpx;
  border-radius: 12rpx;
  font-weight: bolder;
}


.view-title{
  width: 100%;
  margin-top: 18rpx;
  display: flex;
  position: relative;
  flex-direction: row;
}
.view-text-1{
  font-size: 26rpx;
  padding-left: 26rpx;
  padding-top: 12rpx;
  font-weight: bolder;
}
.view-text-2{
  font-size: 24rpx;
  display: flex;
  position: absolute;
  right: 0;
  padding-top: 12rpx;
  padding-right: 24rpx;
}